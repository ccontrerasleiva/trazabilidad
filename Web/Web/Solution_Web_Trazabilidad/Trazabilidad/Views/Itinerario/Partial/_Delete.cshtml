@model ViewModels.Models.Itinerario

@using (Html.BeginForm(nameof(Trazabilidad.Controllers.ItinerarioController.Delete), nameof(Resources.Strings.Models.Itinerario), FormMethod.Post, htmlAttributes: new { @id = "modal-element-delete" }))
{
    @Html.HiddenFor(x => x.Id)
    <div class="col-sm-12">
        <h3 class="m-t-none m-b">Eliminar Itinerario</h3>
        <form role="form">
            <h4 class="m-t-none m-b">¿Desea eliminar un Itinerario  ?</h4>
            <div>
                <button class="btn btn-sm btn-primary pull-right m-t-n-xs" type="submit"><strong><i class="fa fa-plus"></i> Eliminar</strong></button>
            </div>
        </form>
    </div>
}